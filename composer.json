{
    "name": "barryvdh/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "^5.5|^6|^7",
        "dompdf/dompdf": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\DomPDF\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Barryvdh\\DomPDF\\Facade"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {"php": "7.0.13"},
        "github.com": "bec2e071fcae70e1e722f64cd82eaf3a5e1a5e78",
        "github-oauth": {"github.com": "bec2e071fcae70e1e722f64cd82eaf3a5e1a5e78"}
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/rjpl92/dompdf"
        }
    ]

}
